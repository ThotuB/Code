(defvar lst1 '(1 2 3 4 5))

; First element
(print (car lst1))

; Rest of the list
(print (cdr lst1))

; Second element
(print (cadr lst1))

; Third element
(print (caddr lst1))

; Fourth element
(print (cadddr lst1))


